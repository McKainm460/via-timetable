{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/essential", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-else-return": 0,
    "no-restricted-syntax": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "guard-for-in": 0,
    "arrow-parens": 0,
    "array-callback-return": 0,
    "consistent-return": 0,
    "prettier/prettier": ["error"]
  }
}
